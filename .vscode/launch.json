    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Win32) Launch Debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/win32/build_debug/shimmer_win32_c_ocoa.exe",
            "args": ["--test", "NSApplication.dump.txt" ],
            //"args": ["--test", "NSString.dump.txt" ],
            //"args": ["--test", "NSWindow.dump.txt" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/win32/",
            "environment": [],
        },
        {
            "name": "(Win32) Launch Release",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/win32/build_release/shimmer_win32_c_ocoa.exe",
            "args": ["--test", "NSApplication.dump.txt" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/win32/",
            "environment": [],
        },
    ]
