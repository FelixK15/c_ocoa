{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build C-ocoa API Generator - DEBUG",
            "type": "shell",
            "options": {
                "cwd": "win32"
            },
            "windows": {
                "command": "build_clang.bat",
                "args": [
                    "debug"
                ]
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
        },
        {
            "label": "Build C-ocoa API Generator - DEBUG",
            "type": "shell",
            "options": {
                "cwd": "win32"
            },
            "windows": {
                "command": "build_clang.bat",
                "args": [
                    "release"
                ]
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        }
    ]
}